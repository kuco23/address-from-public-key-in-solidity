    "test": "yarn hardhat test",
    "c": "yarn compile",
    "compile": "yarn hardhat compile && yarn typechain-ethers-v5 && yarn typechain-truffle-v5",
    "typechain-prepare": "env bash scripts/remove-flattened-duplicates.sh",
    "typechain-ethers-v5": "yarn typechain --target=ethers-v5 --out-dir typechain \"artifacts/!(build-info|flattened)/**/+([a-zA-Z0-9_]).json\"",
    "typechain-truffle-v5": "yarn typechain --target=truffle-v5 --out-dir typechain-truffle \"artifacts/!(build-info|flattened)/**/+([a-zA-Z0-9_]).json\""
  },
